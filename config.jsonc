{
  "reload_style_on_change": true,
  "layer": "top",
  "position": "top",
  "spacing": 0,
  "height": 26,
  "modules-left": [
    "custom/omarchy",
    "hyprland/workspaces",
    "custom/todo"
  ],
  "modules-center": [
    "clock",
    "custom/update",
    "custom/voxtype",
    "custom/screenrecording-indicator"
  ],
  "modules-right": [
    // "group/tray-expander",
    "hyprland/window",
    "custom/timer",
    "bluetooth",
    "network",
    "pulseaudio",
    "cpu",
    "battery",
    "custom/nightlight",
  ],
  "hyprland/workspaces": {
