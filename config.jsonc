      "warning": 20,
      "critical": 10
    }
  },
  "bluetooth": {
    "format": "ïŠ”",
    "format-off": "ó°‚²",
    "format-disabled": "ó°‚²",
    "format-connected": "ó°‚±",
    "format-no-controller": "",
    "tooltip-format": "Devices connected: {num_connections}",
    "on-click": "omarchy-launch-bluetooth"
  },
  "pulseaudio": {
    "format": "{icon}",
    "on-click": "omarchy-launch-audio",
    "on-click-right": "pamixer -t",
    "tooltip-format": "Playing at {volume}%",
    "scroll-step": 5,
    "format-muted": "î»¨",
    "format-icons": {
      "headphone": "ï€¥",
      "default": [
        "ï€¦",
        "ï€§",
        "ï€¨"
      ]
    }
  },
  "group/tray-expander": {
    "orientation": "inherit",
    "drawer": {
      "transition-duration": 600,
      "children-class": "tray-group-item"
    },
    "modules": [
      "custom/expand-icon",
      "tray"
    ]
  },
  "custom/expand-icon": {
    "format": "ï“",
    "tooltip": false,
    "on-scroll-up": "",
    "on-scroll-down": "",
    "on-scroll-left": "",
    "on-scroll-right": ""
  },
  "custom/screenrecording-indicator": {
    "on-click": "omarchy-cmd-screenrecord",
    "exec": "$OMARCHY_PATH/default/waybar/indicators/screen-recording.sh",
    "signal": 8,
    "return-type": "json"
  },
  "custom/voxtype": {
    "exec": "omarchy-voxtype-status",
    "return-type": "json",
    "format": "{icon}",
    "format-icons": {
      "idle": "",
      "recording": "ó°¬",
      "transcribing": "ó°”Ÿ"
    },
    "tooltip": true,
    "on-click-right": "omarchy-voxtype-config",
    "on-click": "omarchy-voxtype-model"
  },
  "tray": {
    "icon-size": 12,
    "spacing": 17
  },
  "custom/nightlight": {
    "exec": "sh -c 'pgrep -x hyprsunset >/dev/null && echo \"{\\\"text\\\":\\\"ó°–”\\\"}\" || echo \"{\\\"text\\\":\\\"ó°–™\\\"}\"'",
    "restart-interval": 1,
    "return-type": "json",
    "format": "{text}",
    "on-click": "/home/jaidev/.local/bin/hyprsunset-toggle"
  },
  "custom/timer": {
    "exec": "~/.config/waybar/scripts/timer.sh",
    "return-type": "json",
    "format": "{}",
    "on-click": "~/.config/waybar/scripts/timer.sh click",
    "on-click-right": "~/.config/waybar/scripts/timer.sh right",
    "on-click-middle": "~/.config/waybar/scripts/timer.sh middle",
    "on-scroll-up": "~/.config/waybar/scripts/timer.sh down",
    "on-scroll-down": "~/.config/waybar/scripts/timer.sh up",
    "tooltip": true,
    "escape": true
  },
  "custom/todo": {
    "exec": "$HOME/.config/waybar/todo-scripts/todo.py",
    "return-type": "json",
    "interval": 20,
    "signal": 13,
    "on-click": "$HOME/.config/waybar/todo-scripts/todo.py toggle-current --signal 13",
    "on-click-right": "hyprctl dispatch exec \"[float;size 60% 60%;center] xdg-terminal-exec $HOME/.config/waybar/todo-scripts/todo.py tui --signal 13\"",
    "on-click-middle": "$HOME/.config/waybar/todo-scripts/todo.py reset --signal 13",
    "tooltip": true
  },
  "hyprland/window": {
    "format": "{}", // Can also use "{title}"
    "max-length": 50,
    "separate-outputs": true,
    "rewrite": {
      "(.*) â€” Mozilla Firefox": "ðŸŒŽ $1",
      "(.*) - zsh": "> [$1]"
    }
  }
}